<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:f="sap.ui.layout.form" controllerName="project4.controller.View1">
    <Page id="page" title="{i18n>title}" class="customHeader">
        <content>
            <f:SimpleForm id="SimpleFormToolbar" editable="true" layout="ResponsiveGridLayout" labelSpanXL="1" labelSpanL="3" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1" singleContainerFullSize="false" ariaLabelledBy="Title1">
                <f:toolbar>
                    <Toolbar id="TB1">
                        <Title id="Title1" text="{i18n>Simpleform}"/>
                        <ToolbarSpacer id="_IDGenToolbarSpacer"/>
                        <Button id="_IDGenButton1" text="{i18n>Edit}" type="Emphasized" icon="sap-icon://edit"/>
                        <Button id="_IDGenButton2" text="{i18n>Save}" type="Accept" press="saveFunc" icon="sap-icon://save"/>
                    </Toolbar>
                </f:toolbar>
                <f:content>
                    <Label id="_IDGenLabel1" text="{i18n>TravelID}" class="Label"/>
                    <Input id="_IDGenInput1" value="tralala" maxLength="10" editable="false"/>
                    <Label
                    id="_IDGenLabel2" text="{i18n>AgencyID}" required="true"/>
                    <!-- <Input id="agencyID" value="{AgencyId}" showValueHelp="true" valueHelpRequest=".onAgencyValueHelp"/> -->
                    <Input id="agID" value="{AgencyId}" showValueHelp="true" valueHelpRequest="f4HelpForAgencyId"/>
                    <Label
                    id="_IDGenLabel3" text="{i18n>CustomerID}" required="true"/>
                    <!-- <Input id="cusID" value="{CustomerId}" showValueHelp="true" valueHelpRequest=".onCustomerValueHelp"/> -->
                    <Input id="cusID" value="{CustomerId}" showValueHelp="true" valueHelpRequest="f4HelpForCustomerId"/>
                    <Label id="_IDGenLabel4" text="{i18n>StartDate}"/>
                    <Input id="_IDGenInput4" value=""/>
                    <Label id="_IDGenLabel5" text="{i18n>EndDate}"/>
                    <Input id="_IDGenInput5" value=""/>
                    <Label id="_IDGenLabel6" text="{i18n>Description}"/>
                    <Input id="_IDGenInput6" value=""/>
                    <Label id="_IDGenLabel7" text="{i18n>Total Price}"/>
                    <Input id="_IDGenInput7" value="" type="Number"/>
                </f:content>
            </f:SimpleForm>
            <List id="_IDGenList1" items="{oJsonForView>/}" headerText="Customer">
                <ObjectListItem id="_IDGenObjectListItem" title="{oJsonForView>LastName}" type="Active" press="onListItemPress" number="{oJsonForView>CustomerId}">
                    <ObjectAttribute id="_IDGenObjectAttribute" text="{oJsonForView>AgencyId_Text}"/>
                    <ObjectAttribute
                    id="_IDGenObjectAttribute2" text="{path: 'oJsonForView>BeginDate', formatter: '.formatter.dateToFormat'}"/>
                    <!-- <ObjectAttribute id="_IDGenObjectAttribute3" text="{oJsonForView>EndDate}"/> -->
                    <ObjectAttribute id="_IDGenObjectAttribute3" text="{path: 'oJsonForView>EndDate', formatter: '.formatter.dateToFormat'}"/>

                </ObjectListItem>

            </List>

        </content>
    </Page>
</mvc:View>
